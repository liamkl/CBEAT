    <summaries>
      <billSummaries>
        <item>
          <updateDate>2013-04-18T04:00:00Z</updateDate>
          <actionDate>2013-04-18</actionDate>
          <name>Introduced in House</name>
          <text><![CDATA[<p>Employee Hardship Financial Relief Act of 2013 - Amends the Internal Revenue Code to permit federal employees who are furloughed due to sequestration to make penalty-free distributions from their Thrift Savings Account Fund but only to the extent that such distributions do not exceed the aggregate reduction in compensation of such employees resulting from such furlough.</p>]]></text>
          <actionDesc>Introduced in House</actionDesc>
          <versionCode>00</versionCode>
          <lastSummaryUpdateDate>2013-05-22T19:54:30Z</lastSummaryUpdateDate>
        </item>
      </billSummaries>
