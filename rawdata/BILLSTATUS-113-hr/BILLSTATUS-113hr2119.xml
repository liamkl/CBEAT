      <item>
        <chamberCode />
        <title>To amend title 38, United States Code, to improve the opportunity for veterans to use video conferencing for hearings before the Board of Veterans' Appeals.</title>
        <titleType>Official Title as Introduced</titleType>
        <parentTitleType />
        <chamberName />
