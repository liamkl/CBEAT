    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2013-04-10</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <text><![CDATA[<p>Dave Thomas Adoption Act of 2013 - Amends the Internal Revenue Code to allow penalty-free withdrawals, up to $10,000, from individual retirement accounts (IRAs) if used to pay adoption expenses, and unlimited withdrawals for expenses related to the adoption of a special needs child. </p>]]></text>
          <updateDate>2013-04-10T04:00:00Z</updateDate>
          <lastSummaryUpdateDate>2013-04-26T18:12:39Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
